{
  "name": "reunify",
  "version": "2.0.8",
  "description": "A simple framework for server-rendered React applications",
  "author": "Mike Bild <mike.bild@gmail.com>",
  "homepage": "https://github.com/reunify/reunify#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reunify/reunify.git"
  },
  "bugs": {
    "url": "https://github.com/reunify/reunify/issues"
  },
  "keywords": [
    "isomorphic",
    "react",
    "server",
    "render"
  ],
  "bin": {
    "reunify": "./bin/reunify"
  },
  "scripts": {
    "start": "./bin/reunify",
    "build": "npm run build-client && npm run build-server",
    "build-server": "babel -o ./app.js --presets 'react,es2015,stage-0' routes.js server.js",
    "build-client": "webpack ./client.js ./static/bundle.js"
  },
  "devDependencies": {},
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "classnames": "^2.2.5",
    "cross-spawn": "^5.0.1",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "glamor": "^2.20.22",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "mkdirp-then": "^1.2.0",
    "mz": "^2.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "webpack": "^1.14.0",
    "webpack-middleware": "^1.5.1"
  }
}

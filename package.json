{
  "name": "reunify",
  "version": "4.9.1",
  "description":
    "Simplified creation of universal ReactJS progressive web applications.",
  "author": "Mike Bild <mike@codecommission.com>",
  "homepage": "https://github.com/codecommission/reunify#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codecommission/reunify.git"
  },
  "bugs": {
    "url": "https://github.com/codecommission/reunify/issues"
  },
  "keywords": [
    "universal",
    "react",
    "server",
    "render",
    "progressive",
    "pwa",
    "export"
  ],
  "main": "server.js",
  "bin": {
    "reunify": "./bin/reunify"
  },
  "scripts": {
    "start": "NODE_ENV=development ./bin/reunify serve",
    "start:production": "NODE_ENV=production ./bin/reunify serve",
    "build": "npm run build-client && npm run build-server",
    "build-server":
      "babel -o ./app.js --presets 'react,es2015,stage-0' routes.js server.js",
    "build-client": "webpack ./client.js ./static/bundle.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "boxen": "^1.2.1",
    "chalk": "^2.1.0",
    "clipboardy": "^1.1.2",
    "commander": "^2.9.0",
    "compression": "^1.6.2",
    "css-loader": "^0.28.5",
    "ejs": "^2.5.7",
    "eslint": "^4.5.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^1.1.4",
    "fs-extra": "^4.0.2",
    "isomorphic-fetch": "^2.2.1",
    "node-version": "^1.1.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-cookie": "^2.0.9",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-router": "^3.2.0",
    "react-transform-hmr": "^1.0.4",
    "shelljs": "^0.7.8",
    "style-loader": "^0.19.0",
    "styled-components": "^2.1.2",
    "universal-cookie": "^2.0.9",
    "universal-cookie-express": "^2.0.9",
    "update-notifier": "^2.2.0",
    "url-loader": "^0.6.1",
    "webpack": "^2.7.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-middleware": "^1.5.1"
  }
}

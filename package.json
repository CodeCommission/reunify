{
  "name": "reunify",
  "version": "3.3.3",
  "description": "A simple framework for server-rendered React applications",
  "author": "Mike Bild <mike.bild@gmail.com>",
  "homepage": "https://github.com/codecommission/reunify#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codecommission/reunify.git"
  },
  "bugs": {
    "url": "https://github.com/codecommission/reunify/issues"
  },
  "keywords": [
    "universal",
    "react",
    "server",
    "render"
  ],
  "bin": {
    "reunify": "./bin/reunify"
  },
  "scripts": {
    "start": "./bin/reunify serve",
    "build": "npm run build-client && npm run build-server",
    "build-server": "babel -o ./app.js --presets 'react,es2015,stage-0' routes.js server.js",
    "build-client": "webpack ./client.js ./static/bundle.js"
  },
  "devDependencies": {
    "eslint": "^4.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "boxen": "^1.1.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "clipboardy": "^1.1.2",
    "commander": "^2.9.0",
    "compression": "^1.6.2",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "fs-extra": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "node-version": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "shelljs": "^0.7.8",
    "styled-components": "^2.0.1",
    "update-notifier": "^2.2.0",
    "webpack": "^1.14.0",
    "webpack-middleware": "^1.5.1"
  }
}

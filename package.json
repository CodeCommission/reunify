{
  "name": "reunify",
  "version": "4.3.0",
  "description": "Simplified creation of universal ReactJS progressive web applications.",
  "author": "Mike Bild <mike@codecommission.com>",
  "homepage": "https://github.com/codecommission/reunify#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codecommission/reunify.git"
  },
  "bugs": {
    "url": "https://github.com/codecommission/reunify/issues"
  },
  "keywords": [
    "universal",
    "react",
    "server",
    "render",
    "progressive",
    "pwa",
    "export"
  ],
  "main": "server.js",
  "bin": {
    "reunify": "./bin/reunify"
  },
  "scripts": {
    "start": "NODE_ENV=development ./bin/reunify serve",
    "build": "npm run build-client && npm run build-server",
    "build-server": "babel -o ./app.js --presets 'react,es2015,stage-0' routes.js server.js",
    "build-client": "webpack ./client.js ./static/bundle.js"
  },
  "devDependencies": {
    "eslint": "^4.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "boxen": "^1.2.1",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "clipboardy": "^1.1.2",
    "commander": "^2.9.0",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.7",
    "eslint": "^4.4.0",
    "express": "^4.14.0",
    "fs-extra": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.7",
    "node-version": "^1.1.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-cookie": "^2.0.8",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-router": "^3.0.5",
    "react-transform-hmr": "^1.0.4",
    "shelljs": "^0.7.8",
    "style-loader": "^0.18.2",
    "styled-components": "^2.1.2",
    "universal-cookie": "^2.0.8",
    "universal-cookie-express": "^2.0.6",
    "update-notifier": "^2.2.0",
    "webpack": "^1.14.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-manifest-plugin": "^1.2.1",
    "webpack-middleware": "^1.5.1"
  }
}
